{
    "name": "2seg/numeris-v2",
    "description": "Home of the new website of Numéris.",
    "keywords": ["Numéris", "association", "website"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Eliott de Séguier",
            "email": "eliottdes@gmail.com",
            "role": "Developer"
        },
        {
            "name": "MinhNam Nguyen",
            "email": "nguyen.minhnam@hotmail.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-zip": "*",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "doctrine/dbal": "^2.7",
        "fedeisas/laravel-mail-css-inliner": "^2.2",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.5",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "tymon/jwt-auth": "^1.0.0-rc.2"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
